{
    "git": {
      "commitMessage": "chore(release): release v${version} :tada:",
      "requireBranch": "main",
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  }